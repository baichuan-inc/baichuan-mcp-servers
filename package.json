{
  "name": "baichuan-mcp-servers",
  "version": "0.0.0",
  "private": true,
  "description": "Baichuan MCP Servers Monorepo",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "inspector": "mcp-inspector",
    "clean": "turbo run clean && rimraf node_modules",
    "release": "turbo run build && node ./scripts/publish.mjs --all",
    "pack:mcpb": "node ./scripts/pack-mcpb.mjs"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.19.0",
    "rimraf": "^6.0.1",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=18.0.0"
  }
}